<%= stylesheet_link_tag 'results' %>
<h2>Search Results</h2>

<%if @results_articles.empty? %>
<p>Nothing found.</p>
<%end%>

<table>
<tr>
  <th>Article Name</th>
  <th>Wikipedia Id</th> 
</tr>
	<% @results_articles.each do |article| %>
	<tr>
		<td><%= article.name %></td>
		<td><%= article.idwiki %></td>
	</tr>
	<%end%>
</table>